Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

Host github.com
  HostName github.com
  User git
  PreferredAuthentications publickey
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id

Host github.com-work
  HostName github.com
  User git
  PreferredAuthentications publickey
  IdentitiesOnly yes
  IdentityFile ~/.ssh/work_id

{{- if onepasswordItemExists "op://Personal/SSH Config Additional" }}
{{ onepasswordRead "op://Personal/SSH Config Additional/notesPlain" }}
{{- end }}
